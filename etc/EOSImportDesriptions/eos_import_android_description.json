{
    "source_to_dest": [
        {"src":"SDK/Source/Generated/Android/*.cs", "dest": "com.playeveryware.eos/Runtime/EOS_SDK/Generated/Android/"},
        {"src":"SDK/Source/Generated/Android/Platform/*.cs", "dest": "com.playeveryware.eos/Runtime/EOS_SDK/Generated/Android/Platform/"},

        {"src": "SDK/Bin/Android/dynamic-stdc++/aar/*.aar", "dest": "etc/PlatformSpecificAssets/EOS/Android/dynamic-stdc++/aar/"},
        {"src": "SDK/Bin/Android/static-stdc++/aar/*.aar", "dest": "etc/PlatformSpecificAssets/EOS/Android/static-stdc++/aar/"},

        {"src": "SDK/Include/Android/*.h", "dest": "lib/NativeCode/third_party/eos_sdk/include/Android/"}
    ]
}
